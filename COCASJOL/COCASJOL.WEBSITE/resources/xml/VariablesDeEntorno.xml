<?xml version="1.0" encoding="utf-8" ?>

<paginas>
  <pagina>
    <Name>NotasDePesoEnPesaje</Name>
    <variables>
      <variable>
        <key>NOTA_PORCENTAJEHUMEDADMIN</key>
        <descripcion>El porcentaje de humedad minima para descontar de la nota de peso.</descripcion>
        <type>decimal</type>
      </variable>
      <variable>
        <key>NOTA_TRANSPORTECOOP</key>
        <descripcion>El porcentaje a descontar de la nota de peso por concepto de transporte de café.</descripcion>
        <type>decimal</type>
      </variable>
    </variables>
  </pagina>
  <pagina>
    <Name>NotasDePesoEnCatacion</Name>
    <variables>
      <variable>
        <key>NOTA_PORCENTAJEHUMEDADMIN</key>
        <descripcion>El porcentaje de humedad minima para descontar de la nota de peso.</descripcion>
        <type>decimal</type>
      </variable>
      <variable>
        <key>NOTA_TRANSPORTECOOP</key>
        <descripcion>El porcentaje a descontar de la nota de peso por concepto de transporte de café.</descripcion>
        <type>decimal</type>
      </variable>
    </variables>
  </pagina>
  <pagina>
    <Name>HojasDeLiquidacion</Name>
    <variables>
      <variable>
        <key>HOJAS_CAPITALIZACIONRETN</key>
        <descripcion>El porcentaje de capitalización por retencion en hojas de liquidación.</descripcion>
        <type>int</type>
      </variable>
    </variables>
  </pagina>
  <pagina>
    <Name>HojasDeLiquidacion</Name>
    <variables>
      <variable>
        <key>HOJAS_CUOTAINGRESO</key>
        <descripcion>La cuota de ingreso a pagar para socios nuevos en hojas de liquidación.</descripcion>
        <type>decimal</type>
      </variable>
    </variables>
  </pagina>
  <pagina>
    <Name>HojasDeLiquidacion</Name>
    <variables>
      <variable>
        <key>HOJAS_GASTOADMINISTRACION</key>
        <descripcion>La cantidad de gastos de administración para socios nuevos en hojas de liquidación.</descripcion>
        <type>decimal</type>
      </variable>
    </variables>
  </pagina>

  <pagina>
    <Name>Socios</Name>
    <variables>
      <variable>
        <key>SOCIOS_IMPORTAREXCEL</key>
        <descripcion>Indica si esta habilitada la opción de importar socios.</descripcion>
        <type>int</type>
      </variable>
    </variables>
  </pagina>
</paginas>
